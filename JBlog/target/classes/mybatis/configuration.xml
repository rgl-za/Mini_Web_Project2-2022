<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings> 
		<setting name="cacheEnabled" value="false" /> 
		<setting name="jdbcTypeForNull" value="NULL" /> 
	</settings>
  
  	<typeAliases>
		<typeAlias alias="insertUserVo" type="com.javaex.vo.UserVo"></typeAlias>
		<typeAlias alias="postlist" type="com.javaex.vo.PostVo"></typeAlias>
		<typeAlias alias="catelist" type="com.javaex.vo.CateVo"></typeAlias>
	</typeAliases>
	<mappers>	
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/blog.xml" />
		<mapper resource="mybatis/mappers/cate.xml" />
		<mapper resource="mybatis/mappers/post.xml" />
	</mappers>

</configuration>
 